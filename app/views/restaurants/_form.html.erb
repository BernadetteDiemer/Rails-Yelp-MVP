<%= form_for(restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.label :category %>
  <p>must be one of the following: chinese, japanese, french, belgian, italian</p>
  <%= f.text_field :category %>
  <%= f.submit %>
<% end %>
